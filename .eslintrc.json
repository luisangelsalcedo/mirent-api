{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/extensions": 0,
    "import/prefer-default-export": 0,
    "func-names": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-useless-escape": 0,
    "no-unused-vars": 0,
    "prefer-arrow-callback": 0,
    "no-prototype-builtins": 0
  }
}
